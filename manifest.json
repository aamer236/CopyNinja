{
  "manifest_version": 3,
  "name": "Copy Enabler",
  "version": "1.0",
  "description": "Enable copying on websites that restrict it",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}